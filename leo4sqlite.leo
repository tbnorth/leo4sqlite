# delete-comments

import re

def delete_remarks():
    
    new_lines = []    
    lines = re.split(r'\n', p.b)

    p.b = ''
    
    for line in lines:
        if re.search(r'.*(#.*)', line):
            line = re.sub(r'#.*', '', line)
        if line != '\n':
            p.b += line + '\n'    
        else:
            p.b += line
        
    p.b = p.b[:-1]
    
delete_remarks()
