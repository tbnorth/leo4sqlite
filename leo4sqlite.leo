def code_folding():
    
    import re
    import leo.core.leoGlobals as g
    w = c.frame.body.wrapper
    
    s = ''
    string = ''    
    begin = 'n'
    end = 'n'
    new_txt = ''
    chars = {}
    char = ''
    
    p = c.p
    c.selectPosition(p)
       
    i = w.getInsertPoint()
 
    while i &gt; 0:
            
        c1 = w.get(i, i+1)
        c2 = w.get(i+1, i+2)
        c3 = w.get(i+2, i+3)
        c4 = w.get(i+3, i+4)
        
        # i -= 4
        
        chars = c1 + c2 + c3 + c4
    
        if re.match(r'#-[0-9][0-9]', chars):
            begin = 'y'
            s = chars
            i -= 4
        
        if re.match(r'#\+[0-9][0-9]', chars):
            s = chars + s + '\n'
            i -= 4
            return s
        
        else:
            s = chars + s
        i -= 4
        
s = code_folding()
g.es(s)
