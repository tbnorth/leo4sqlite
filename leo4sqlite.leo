import re

def delete_comments():
    
    new_lines = []
        
    p = c.p
    
    lines = re.split(r'\n', p.b)
    
    p.b = ''
    
    i = 0    
    for line in lines:
        if re.search(r'.*(#.*)', line):
            line = re.sub(r'#.*', '', line)
        if line != '\n':
            p.b += line + '\n'
        if line == '':
            continue
        i += 1                                
        
delete_comments()
